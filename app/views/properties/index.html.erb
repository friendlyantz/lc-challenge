<p id="notice"><%= notice %></p>

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>Lc</th>
      <th>Lga code</th>
      <th>Council property number</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.lc_id %></td>
        <td><%= property.lgasfile.code %><%= link_to "go to this LGA", lgasfile_path(property.lgasfile) %></td>
        <td><%= property.council_property_number %></td>
        <td><%= property.longitude %></td>
        <td><%= property.latitude %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
